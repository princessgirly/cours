var compteur;
for (compteur = 1; compteur <= 5; compteur++) {
    console.log(compteur);
} console.log("Fin programme");


/* resultat :
1
2
3
4
5
Fin programme
*/


for (var i = 1; i < 5; i++) {
   console.log("J'ai faim !");
}

/*resultat :
4 J'ai faim !
*/



for (var compteur = 1; compteur <= 5; compteur++) {
    console.log(compteur);
    compteur++; // La variable est modifiÃ©e dans le corps de la boucle
}

//resultat
1
3
5
